{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@now/next"
      },
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  